{
  "name": "nest-starter",
  "version": "0.1.0",
  "description": "Hackathon starter project for NestJS.",
  "repository": {
    "type": "git",
    "url": "https://github.com/moneebullah25/nest-starter"
  },
  "pnpm": {
    "overrides": {
      "form-data": "4.0.4"
    }
  },
  "author": "Muneeb Ullah",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "clean": "rimraf dist",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "prepare": "sh -c 'command -v husky >/dev/null 2>&1 && husky || true'",
    "start": "pnpm exec dotenv -e .env.dev -- nest start",
    "start:dev": "pnpm exec dotenv -e .env.dev -- nest start --watch",
    "start:test": "pnpm exec dotenv -e .env.test -- nest start --watch",
    "start:prod": "dotenv -e .env.prod -- node dist/src/main",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "test": "dotenv -e .env.test -- jest",
    "test:watch": "dotenv -e .env.test -- jest --watch",
    "test:coverage": "dotenv -e .env.test -- jest --coverage",
    "test:runner": "dotenv -e .env.test -- node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "dotenv -e .env.test -- jest --config ./test/jest-e2e.json",
    "migrate:dev": "dotenv -e .env.dev -- prisma migrate dev",
    "migrate:dev:create": "dotenv -e .env.dev -- prisma migrate dev --create-only",
    "migrate:dev:pull": "dotenv -e .env.dev -- prisma db pull",
    "migrate:dev:reset": "dotenv -e .env.dev -- prisma migrate reset",
    "migrate:deploy:prod": "dotenv -e .env.prod -- prisma migrate deploy",
    "migrate:deploy:dev": "dotenv -e .env.dev -- prisma migrate deploy --preview-feature",
    "migrate:status": "prisma migrate status --preview-feature",
    "migrate:resolve": "prisma migrate resolve --preview-feature",
    "lint:staged": "lint-staged",
    "prisma:studio": "prisma studio",
    "prisma:generate": "prisma generate",
    "prisma:generate:watch": "prisma generate --watch",
    "prisma:seed:dev": "dotenv -e .env.dev -- ts-node --transpile-only prisma/seed.ts",
    "prisma:seed:test": "dotenv -e .env.test -- ts-node --transpile-only prisma/seed.ts",
    "prisma:seed:prod": "dotenv -e .env.prod -- ts-node --transpile-only prisma/seed.ts",
    "docker:dev": "docker compose -f docker-compose.dev.yml --env-file ./.env.dev up",
    "docker:dev:build": "docker compose -f docker-compose.dev.yml --env-file ./.env.dev build --no-cache",
    "docker:dev:down": "docker compose -f docker-compose.dev.yml down -v",
    "docker:prod": "docker compose -f docker-compose.prod.yml --env-file ./.env.prod up -d",
    "docker:prod:build": "docker compose -f docker-compose.prod.yml --env-file ./.env.prod build --no-cache",
    "docker:prod:down": "docker compose -f docker-compose.prod.yml down -v"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "pnpm run lint",
      "cross-env NODE_ENV=test eslint --cache --fix"
    ],
    "{*.json,.prettierrc}": [
      "prettier --ignore-path .eslintignore --parser json --write"
    ],
    "*.{html,md,yml}": [
      "prettier --ignore-path .eslintignore --single-quote --write"
    ]
  },
  "dependencies": {
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/throttler": "6.4.0",
    "@prisma/client": "6.16.2",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dotenv": "^17.2.2",
    "dotenv-cli": "10.0.0",
    "dotenv-expand": "^12.0.3",
    "helmet": "^8.1.0",
    "nanoid": "5.1.6",
    "nodemailer": "^7.0.6",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.2",
    "request-ip": "^3.3.0",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.2",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/eslintrc": "^3.3.1",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.7",
    "@nestjs/testing": "^11.1.6",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.5.2",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "cross-env": "^10.0.0",
    "eslint": "^9.36.0",
    "eslint-config-love": "^125.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jest": "^30.1.3",
    "jest-extended": "6.0.0",
    "jest-mock-extended": "4.0.0",
    "lint-staged": "^16.2.0",
    "prettier": "^3.6.2",
    "prisma": "6.16.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.4",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.1"
  },

  "jest": {
    "moduleFileExtensions": ["js", "json", "ts"],
    "roots": ["src", "test"],
    "setupFilesAfterEnv": ["jest-extended", "./jest.setup.ts"],
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "transformIgnorePatterns": ["/node_modules/"],
    "moduleNameMapper": {
      "^nanoid$": "<rootDir>/test/__mocks__/nanoid.ts"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
